{
    "name": "@virmator/docs",
    "version": "13.0.0",
    "description": "Default TS docs plugin for virmator.",
    "keywords": [
        "virmator",
        "deps",
        "upgrade",
        "dependencies"
    ],
    "homepage": "https://github.com/electrovir/virmator",
    "bugs": {
        "url": "https://github.com/electrovir/virmator/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/electrovir/virmator.git"
    },
    "license": "(MIT or CC0 1.0)",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "type": "module",
    "main": "src/docs.ts",
    "module": "src/docs.ts",
    "types": "src/docs.ts",
    "scripts": {
        "compile": "virmator compile",
        "test": "tsx --test --experimental-test-snapshots --test-reporter spec 'src/**/*.test.ts'",
        "test:coverage": "c8 --config configs/c8.config.json npm test",
        "test:update": "tsx --test --experimental-test-snapshots --test-update-snapshots --test-reporter spec 'src/**/*.test.ts'"
    },
    "dependencies": {
        "@augment-vir/common": "^29.3.0",
        "@augment-vir/node-js": "^29.3.0",
        "@virmator/core": "^13.0.0",
        "chalk": "^5.3.0",
        "mri": "^1.2.0",
        "run-time-assertions": "^1.5.2"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "esbuild": "^0.23.0",
        "markdown-code-example-inserter": "^1.0.0",
        "typedoc": "^0.26.5"
    },
    "publishConfig": {
        "access": "public"
    }
}
