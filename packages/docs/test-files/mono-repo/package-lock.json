{
    "name": "mono-repo",
    "lockfileVersion": 3,
    "requires": true,
    "packages": {
        "": {
            "workspaces": [
                "packages/*"
            ]
        },
        "node_modules/a": {
            "resolved": "packages/a",
            "link": true
        },
        "node_modules/b": {
            "resolved": "packages/b",
            "link": true
        },
        "packages/a": {},
        "packages/b": {}
    }
}
